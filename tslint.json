{
  "extends": "./strict",
  "rules": {
    "no-var-requires": false,
    "no-require-imports": false,
    "no-magic-numbers": false,
    "no-duplicate-string": false,
    "missing-jsdoc": false,
    "object-literal-sort-keys": false,
    "chai-vague-errors": false,
    "no-relative-imports": false,
    "min-class-cohesion": false,
    "no-unsafe-any": false
  }
}
